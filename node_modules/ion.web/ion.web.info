<cfg:configuration xmlns:cfg="http://www.iontrading.com/ionweb/config" xmlns:info="urn:schemas-iontrading-com:libraryinfo.v1">
  <xs:schema targetNamespace="http://www.iontrading.com/ionweb/config/shell" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  	<!-- Define string -->
    <xs:simpleType name="ionString">
      <xs:annotation>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/variableType">STRING</xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string" />
    </xs:simpleType>

    <!-- The Low/Med/High enumerated for Web logging -->
    <xs:simpleType name="logLevelEnum">
      <xs:annotation>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/variableType">ENUMERATION</xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
        <!-- Low value -->
        <xs:enumeration value="low">
          <xs:annotation>
            <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">Low</xs:appinfo>
            <xs:documentation>Standard logging (default, for production environments)</xs:documentation>
          </xs:annotation>
        </xs:enumeration>

        <!-- Medium value -->
        <xs:enumeration value="med">
          <xs:annotation>
            <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">Medium</xs:appinfo>
            <xs:documentation>Verbose logging</xs:documentation>
          </xs:annotation>
        </xs:enumeration>

        <!-- High value -->
        <xs:enumeration value="high">
          <xs:annotation>
            <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">High</xs:appinfo>
            <xs:documentation>Debug logging</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>

  	<!-- Define a boolean -->
    <xs:simpleType name="booleanEnum">
      <xs:annotation>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/variableType">BOOLEAN</xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:boolean">

        <xs:enumeration value="true">
          <xs:annotation>
            <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">True</xs:appinfo>
            <xs:documentation>The feature is enabled.</xs:documentation>
          </xs:annotation>
        </xs:enumeration>

        <xs:enumeration value="false">
          <xs:annotation>
            <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">False</xs:appinfo>
            <xs:documentation>The feature is disabled.</xs:documentation>
          </xs:annotation>
        </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>

    <!-- The remote log level -->
    <xs:element name="logFileLevel" type="logLevelEnum">
      <xs:annotation>
        <xs:appinfo source="jsclient">
          <cfg:jsonpath>["shell", "logLevel"]</cfg:jsonpath>
        </xs:appinfo>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">Log Level</xs:appinfo>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayOrder">1</xs:appinfo>
        <xs:documentation>The log level of the application</xs:documentation>
      </xs:annotation>
    </xs:element>

    <!-- Enable/disable Grid Startup Perf component (for demos) -->
    <xs:element name="lockedWorkspace" type="booleanEnum">
      <xs:annotation>
        <xs:appinfo source="jsclient">
          <cfg:jsonpath>["shell", "lockedWorkspace"]</cfg:jsonpath>
        </xs:appinfo>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">Enable Locked Workspace</xs:appinfo>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayOrder">2</xs:appinfo>
        <xs:documentation>Lock the workspace limiting the actions the user can do. By default this means that it will not be possible to create local dashboards, edit dashboards, import/export dashboards, show history, etc...</xs:documentation>
      </xs:annotation>
    </xs:element>

    <!-- Default workspace to use -->
    <xs:element name="loadWorkspace" type="ionString">
      <xs:annotation>
        <xs:appinfo source="jsclient">
          <cfg:jsonpath>["shell", "loadWorkspace"]</cfg:jsonpath>
        </xs:appinfo>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayName">Load a predefined Workspace</xs:appinfo>
        <xs:appinfo source="http://www.iontrading.com/config/Schema/DisplayOrder">3</xs:appinfo>
        <xs:documentation>If set, forces the application to load the specified workspace (list of dashboards) at startup. The available workspaces should be defined in the <i>config.json</i> file. If specified alongside with <i>lockedWorkspace</i>, the user will not be entitled to open other dashboards or close the ones opened at startup.</xs:documentation>
      </xs:annotation>
    </xs:element>

  </xs:schema>

  <default xmlns="" xmlns:sh="http://www.iontrading.com/ionweb/config/shell">
    <sh:logFileLevel>low</sh:logFileLevel>
    <sh:lockedWorkspace>false</sh:lockedWorkspace>
    <sh:loadWorkspace></sh:loadWorkspace>
  </default>

</cfg:configuration>
